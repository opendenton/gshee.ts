{
  "name": "gshee.ts",
  "version": "1.0.0",
  "description": "A simple library for retrieving data from Google Sheets",
  "scripts": {
    "build": "babel src --out-dir lib --extensions '.ts'",
    "build:types": "tsc --emitDeclarationOnly",
    "docs": "typedoc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opendenton/gshee.ts.git"
  },
